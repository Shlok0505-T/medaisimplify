{
  "version": 2,
  "builds": [
    {
      "src": "Main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "Main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "./"
  },
  "functions": {
    "Main.py": {
      "maxDuration": 30
    }
  }
}
